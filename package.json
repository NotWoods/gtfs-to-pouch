{
  "name": "gtfs-to-pouch",
  "version": "2.0.1",
  "description": "Convert a GTFS schedule into PouchDB databases",
  "repository": "NotWoods/gtfs-to-pouch",
  "main": "dist/gtfs-to-pouch.js",
  "module": "es/gtfs-to-pouch.js",
  "types": "es/gtfs-to-pouch.d.ts",
  "license": "MIT",
  "bin": {
    "gtfs-to-pouch": "./dist/dump/cmd.js"
  },
  "devDependencies": {
    "@types/denodeify": "^1.2.30",
    "@types/geojson": "^1.0.2",
    "@types/jest": "^19.2.4",
    "@types/jszip": "0.0.32",
    "@types/moment-range": "^3.0.0",
    "@types/node": "^7.0.7",
    "@types/pouchdb": "^5.4.28",
    "jest": "^20.0.4",
    "pouchdb-adapter-memory": "^6.2.0",
    "ts-jest": "^20.0.4",
    "typescript": "^2.3.2"
  },
  "dependencies": {
    "csv-to-pouch": "^3.1.1",
    "denodeify": "^1.2.1",
    "is-stream": "^1.1.0",
    "jszip": "^3.1.3",
    "minimist": "^1.2.0",
    "pouchdb": "^6.2.0",
    "promise-stream-utils": "^1.0.9",
    "query-pouch-gtfs": "^2.0.3"
  },
  "scripts": {
    "build-es": "tsc --module es2015 --outDir es",
    "build-cjs": "tsc --module commonjs --outDir dist",
    "prepare": "npm run build-es && npm run build-cjs",
    "test": "jest"
  },
  "files": [
    "dist",
    "es"
  ],
  "keywords": [
    "gtfs",
    "pouchdb",
    "transit",
    "sync",
    "database",
    "transportation"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  }
}
