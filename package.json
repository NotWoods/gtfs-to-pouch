{
  "name": "gtfs-to-pouch",
  "version": "1.1.0",
  "description": "Converts a GTFS file to PouchDB dumpfiles",
  "repository": "NotWoods/gtfs-to-pouch",
  "main": "dist/dump/main.js",
  "module": "es/dump/main.js",
  "bin": "./dist/dump/cmd.js",
  "devDependencies": {
    "@types/denodeify": "^1.2.30",
    "@types/jszip": "0.0.32",
    "@types/moment-range": "^3.0.0",
    "@types/node": "^7.0.7",
    "@types/pouchdb": "^5.4.28",
    "typescript": "^2.3.2"
  },
  "dependencies": {
    "csv-to-pouch": "^2.0.3",
    "denodeify": "^1.2.1",
    "docuri": "^4.2.2",
    "is-stream": "^1.1.0",
    "jszip": "^3.1.3",
    "minimist": "^1.2.0",
    "moment": "^2.18.1",
    "moment-range": "^3.0.3",
    "pouchdb-adapter-memory": "^6.1.2",
    "pouchdb-load": "^1.4.6",
    "pouchdb-replication-stream": "^1.2.9",
    "promise-stream-utils": "^1.0.9"
  },
  "scripts": {
    "build-es": "tsc --module es2015 --outDir es || exit 0",
    "build-cjs": "tsc --module commonjs --outDir dist || exit 0",
    "prepare": "npm run build-es && npm run build-cjs"
  },
  "files": [
    "dist",
    "es",
    "index.d.ts"
  ]
}
